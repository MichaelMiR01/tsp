tsp::proc cat {file} {
	#tsp::procdef void -args string
	#tsp::boolean is_eof
	#tsp::string f buf
	set f [open $file r]
        set buf [gets $f]
        set is_eof [eof $f]
	while {! $is_eof} { puts $buf ; set buf [gets $f] ; set is_eof [eof $f]  }
	close $f
}

proc run_cat {} {
    global argv
    fconfigure stdout -buffering full -translation binary 
    foreach file $argv {
	    cat $file
    }
}
